    {%- set first = true -%}
    {%- for script in collections.published_scripts -%}
      {%- if script.data.series == "school-bcba" -%}
        {%- if not first %},{% endif -%}
        {
            id: {{ script.data.step }},
            title: "{{ script.data.title }}",
            videoUrl: "{{ script.data.videoUrl }}",
            content: `{{ script.templateContent | safe | replace('`', '\\`') }}`,
            series: "{{ script.data.series }}"
        }
        {%- set first = false -%}
